public List<apxio__Student__c> selectStudentsWithoutContactInfo() {
    // code here
    List<apxio__Student__c> = [SELECT Id,Name,apxio__Email__c,apxio__Phone__c from apxio__Student__c where (apxio__Email__c= null OR apxio__Phone__c = null) AND apxio__Active__c = true order By Name ASC];
    return records; 
}